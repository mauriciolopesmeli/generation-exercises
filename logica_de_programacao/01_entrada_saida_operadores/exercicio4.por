programa {
  funcao inicio() {
    inteiro num1, num2, num3, num4
    real diferenca

    escreva("Informe o número 1: ")
    leia(num1)
    escreva("Informe o número 2: ")
    leia(num2)
    escreva("Informe o número 3: ")
    leia(num3)
    escreva("Informe o número 4: ")
    leia(num4)

    diferenca = (num1 * num2) - (num3 * num4)

    escreva("Diferença: ", diferenca)
  }
}
